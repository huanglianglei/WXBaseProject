/* components/move-item/move-item.wxss */

.move-item-view{
  display: flex;
  flex-direction: column;
  width: 200rpx;
}
.move-item-view image{
  width: 100%;
  height: 270rpx;
}
.move-item-view text{
  margin-top: 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-wrap: break-all;
  font-size: 28rpx;
}
.star-view{
  display: flex;
  align-items: baseline;
}
.star-view l-star{
  background-color: #fff;
}
.star-view text{
  margin-left: 10rpx;
  font-size: 24rpx;
  height: 36rpx;
}
